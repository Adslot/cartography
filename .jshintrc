{
    "maxerr"        : 50,      // {int} Maximum error before stopping

    // Enforcing
    "undef"         : true,    // true: Require all non-global variables to be declared (prevents global leaks)
    "noempty"       : true,    // true: Prohibit use of empty blocks
    "nonew"         : true,    // true: Prohibit use of constructors for side-effects (without assignment)
    "latedef"       : true,    // true: Require variables/functions to be defined before being used
    "trailing"      : true,    // true: Prohibit trailing whitespaces
    "indent"        : 2,       // {int} Number of spaces to use for indentation
    "camelcase"     : true,    // true: Identifiers must be in camelCase
    "unused"        : false,   // true: Require all defined variables be used
    "eqeqeq"        : false,   // true: Require triple equals (===) for comparison
    "bitwise"       : false,   // true: Prohibit bitwise operators (&, |, ^, etc.)
    "curly"         : false,   // true: Require {} for every new block or scope
    "forin"         : false,   // true: Require filtering for..in loops with obj.hasOwnProperty()
    "immed"         : true,    // true: Require immediate invocations to be wrapped in parens e.g. `(function () { } ());`
    "newcap"        : false,   // true: Require capitalization of all constructor functions e.g. `new F()`
    "noarg"         : false,   // true: Prohibit use of `arguments.caller` and `arguments.callee`
    "plusplus"      : false,   // true: Prohibit use of `++` & `--`
    "quotmark"      : false,   // {"single"/"double"/true/false}: Quotation mark consistency
    "strict"        : false,   // true: Requires all functions run in ES5 Strict Mode
    "maxparams"     : false,   // {int} Max number of formal params allowed per function
    "maxdepth"      : false,   // {int} Max depth of nested blocks (within functions)
    "maxstatements" : false,   // {int} Max number statements per function
    "maxcomplexity" : false,   // {int} Max cyclomatic complexity per function
    "maxlen"        : 120,     // {int} Max number of characters per line

    // Relaxing
    "eqnull"        : true,    // true: Tolerate use of `== null`
    "boss"          : true,    // true: Tolerate assignments where comparisons would be expected
    "expr"          : true,    // true: Tolerate `ExpressionStatement` as Programs
    "loopfunc"      : false,   // true: Tolerate functions being defined in loops
    "proto"         : true,    // true: Tolerate using the `__proto__` property
    "shadow"        : true,    // true: Allows re-define variables later in code e.g. `var x=1; x=2;`
    "sub"           : true,    // true: Tolerate using `[]` notation when it can still be expressed in dot notation
    "asi"           : false,   // true: Tolerate Automatic Semicolon Insertion (no semicolons)
    "evil"          : false,   // true: Tolerate use of `eval` and `new Function()`
    "debug"         : false,   // true: Allow debugger statements e.g. browser breakpoints.
    "moz"           : false,   // true: Allow Mozilla specific syntax (extends and overrides esnext features)
    "globalstrict"  : false,   // true: Allow global "use strict" (also enables 'strict')
    "funcscope"     : false,   // true: Tolerate defining variables inside control statements"
    "iterator"      : false,   // true: Tolerate using the `__iterator__` property
    "esnext"        : false,   // true: Allow ES.next (ES6) syntax (ex: `const`)
    "lastsemic"     : false,   // true: Tolerate omitting a semicolon for the last statement of a 1-line block
    "laxbreak"      : false,   // true: Tolerate possibly unsafe line breakings
    "laxcomma"      : false,   // true: Tolerate comma-first style coding
    "multistr"      : false,   // true: Tolerate multi-line strings
    "scripturl"     : false,   // true: Tolerate script-targeted URLs
    "smarttabs"     : false,   // true: Tolerate mixed tabs/spaces when used for alignment
    "supernew"      : false,   // true: Tolerate `new function () { ... };` and `new Object;`
    "validthis"     : false,   // true: Tolerate using this in a non-constructor function

    // Environments
    "browser"       : true,    // Web Browser (window, document, etc)
    "jquery"        : true,    // jQuery
    "node"          : true,    // Node.js
    "nonstandard"   : true,    // Widely adopted globals (escape, unescape, etc)
    "devel"         : false,   // Development/debugging (alert, confirm, etc)
    "couch"         : false,   // CouchDB
    "dojo"          : false,   // Dojo Toolkit
    "mootools"      : false,   // MooTools
    "prototypejs"   : false,   // Prototype and Scriptaculous
    "rhino"         : false,   // Rhino
    "worker"        : false,   // Web Workers
    "wsh"           : false,   // Windows Scripting Host
    "yui"           : false,   // Yahoo User Interface

    // Legacy
    "nomen"         : false,   // true: Prohibit dangling `_` in variables
    "onevar"        : false,   // true: Allow only one `var` statement per function
    "passfail"      : false,   // true: Stop on first error
    "white"         : false,   // true: Check against strict whitespace and indentation rules

    // Ignore these specific errors/warnings
    "-W092"         : true,    // Bad JSHint: "Wrap the /regexp/ literal in parens to disambiguate the slash operator"
    "-W058"         : true,    // Bad JSHint: "Missing '()' invoking a constructor"

    // Custom Globals
    "predef"        : []
}

